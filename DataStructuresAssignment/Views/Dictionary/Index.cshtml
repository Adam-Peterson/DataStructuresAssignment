
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <!-- Sidebar -->
    <div style="width:25%">
        <h3>Menu</h3>
        <a href="@Url.Action("addOne", "Dictionary")"> Add one item to Dictionary</a><br /><br />
        <a href="@Url.Action("addHuge", "Dictionary")"> Add Huge List of Items to Dictionary</a><br /><br />
        <a href="@Url.Action("Index", "Dictionary")"> Display Dictionary</a><br /><br />
        <a href="@Url.Action("deleteFromStack", "Dictionary")"> Delete from Dictionary</a><br /><br />
        <a href="@Url.Action("clearStack", "Dictionary")"> Clear Dictionary</a><br /><br />
        <a href="@Url.Action("searchStack", "Dictionary")"> Search Dictionary</a><br /><br />
        <a href="@Url.Action("Index",  "Home")">Return to Main Menu</a><br /><br />
    </div>

    <!-- Page Content -->
    <div style="margin-left:25%">

        <div class="w3-container w3-teal">
            <h1>Output</h1>

            @Html.Raw((String)ViewBag.Dictionary)
            @Html.Raw((String)ViewBag.StopWatch)
        </div>



    </div>

</body>
</html>
